<form>
  <div class="form-group">
    <label for="item-idEntUser" class="control-label">id ENT du propriétaire :</label>
    <input class="form-control" id="item-idEntUser" name="idEntUser" type="text" placeHolder="id ENT"/>
  </div>

  <div class="form-group">
    <label for="item-emailUser" class="control-label">email du propriétaire [optionnel] :</label>
    <input class="form-control" id="item-emailUser" name="emailUser" type="text" placeHolder="email"/>
  </div>

  <div class="form-group">
    <label for="item-title" class="control-label">Titre [affiché dans le menu] :</label>
    <input class="form-control" id="item-title" name="title" type="text" placeHolder="Titre"/>
  </div>

  <div class="form-group">
    <label for="item-description" class="control-label">Description :</label>
    <input class="form-control" id="item-description" name="description" type="text" placeHolder="Description"/>
  </div>

  <div class="form-group">
    <label for="item-filter" class="control-label">Filtre :</label>
    <input class="form-control" id="item-filter" name="filter" type="text" placeHolder="Filtre"/>
  </div>

  <div class="form-group">
    <label for="item-rdvTime" class="control-label">Durée d'un RDV (en min) :</label>
    <input class="form-control" id="item-rdvTime" name="rdvTime" type="text" placeHolder="Durée en minutes"/>
  </div>

  <button class="btn btn-success js-submit">Enregistrer</button>
</form>
